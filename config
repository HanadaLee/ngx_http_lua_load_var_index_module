ngx_module_type=HTTP
ngx_module_name=ngx_http_lua_load_var_index_module
ngx_module_srcs=" \
                $ngx_addon_dir/src/ngx_http_lua_load_var_index_module.c \
                "

if [ -z "$NGX_LUA_LOC" ]; then
    ngx_lua_loc="../ngx_lua-*"
else
    ngx_lua_loc="$NGX_LUA_LOC"
fi

ngx_module_incs="$ngx_addon_dir/src $ngx_lua_loc/src"

. auto/module

ngx_addon_name=$ngx_module_name